{
  "expo": {
    "name": "Workout Tracker",
    "slug": "workout-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "workouttracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "description": "Track your workouts, monitor your progress, and stay motivated on your fitness journey with this modern, offline-first workout tracker.",
    "primaryColor": "#007AFF",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neilsmahajan.workouttracker",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app does not use the camera.",
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "NSLocationWhenInUseUsageDescription": "This app does not use location services."
      }
    },
    "android": {
      "package": "com.neilsmahajan.workouttracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d25c005b-04d4-4195-b5b3-e28f0a35dd59"
      }
    },
    "owner": "neilsmahajan"
  }
}
